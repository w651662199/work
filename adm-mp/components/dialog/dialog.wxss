.wx-mask{
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  overflow: hidden;
}
.wx-dialog{
  position: fixed;
  z-index: 5000;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #FFFFFF;
  text-align: center;
  border-radius: 20rpx;
  overflow: hidden;
}
.dialog-redpacket {
  width: 540rpx;
  min-height: 282rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.dialog-redpacket .redpacket-item {
  width: 460rpx;
  height: 94rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.dialog-redpacket .redpacket-item.border-bottom-1px::after{
  background-color: #ddd;
}
.dialog-redpacket .redpacket-item.border-bottom-1px:last-child::after{
  background-color: #fff;
}
.dialog-redpacket .redpacket-item image {
  width: 46rpx;
  height: 46rpx;
}
.dialog-redpacket .redpacket-item image.check {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  right: 20rpx;
}
.dialog-redpacket .redpacket-item text {
  font-size: 15px;
  color: #666666;
  margin-left: 20rpx;
}
.dialog-close {
  position: absolute;
  right: 30rpx;
  top: 20rpx;
  padding: 6rpx;
  font-size: 0;
}
.dialog-close image {
  width: 30rpx;
  height: 30rpx;
}
.border-bottom-1px {
  position: relative;
}
.border-bottom-1px::after{
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  transform: scaleY(0.5);
  transform-origin: 0 0;
}