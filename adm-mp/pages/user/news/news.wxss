page {
  background: #f2f2f2;
}
.news {
  background: #f2f2f2;
  box-sizing: border-box;
  padding: 30rpx 0;
}
.newsLine {
  width:100%;
  height: 1rpx;
  background: #ececec;
  position: fixed;
  top: 0;
}
.contentItem {
  background: #fff;
  margin: 0 22rpx 30rpx 22rpx;
  padding: 24rpx 30rpx;
  border-radius: 10rpx;
}
.newsContent .contentItem:last-child{
  margin-bottom: 0;
}
.itemTop .rebateCount{
  font-size: 28rpx;
  color: #999;
  height: 58rpx;
  line-height: 58rpx;
}
.itemTop .time{
  font-size: 28rpx;
  color: #999;
  height: 58rpx;
  line-height: 58rpx;
}
.itemMiddle {
  font-size: 36rpx;
  color: #666;
  height: 63rpx;
  line-height: 63rpx;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.loadingFail-content{
  background: #f2f2f2;
  padding: 0 24rpx;
}
.loadingFail-box{
  padding: 102rpx 0 0 0;
  margin: 0 auto;
  width: 100%;
  text-align: center;
}
.loadingFail-box image{
  width: 200rpx;
  height: 200rpx;
}
.loadingFail-box .title{
  font-size: 40rpx;
  color: #000;
  margin: 55rpx 0 42rpx 0;
}