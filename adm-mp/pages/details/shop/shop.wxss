@import "../../../components/rebate/rebate.wxss";
.shop_header {
  width: 100%;
  height: 468rpx;
  background: url(data:image/png;base64,) no-repeat #fff;
  background-size: 100% auto;
  background-position: center bottom;
  text-align: center;
}
.shop_header .shop_inner {
  padding-top: 20rpx;
  margin: 0 auto;
  height: 468rpx;
  box-sizing: border-box;
}
.shop_header .shop_inner .shop_logo {
  position: relative;
  width: 130rpx;
  height: 130rpx;
  margin: 0 auto;
  margin-bottom: 38rpx;
  border: 1rpx solid #ddd;
  text-align: center;
}
.shop_header .shop_inner .shop_logo .shop_icon {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 130rpx;
  height: 130rpx;
  margin: auto;
  vertical-align: middle;
}
.shop_header .shop_inner .shop_title  {
  display: block;
  font-size: 48rpx;
  color: #333;
}
.shop_header .shop_inner .collectNum {
  font-size: 24rpx;
  color: #666;
  margin-top: 16rpx;
}
.shop_header .shop_inner .enterShop {
  width: 200rpx;
  height: 56rpx;
  font-size: 26rpx;
  color: #fff;
  background: linear-gradient(to right, #fa1e8c, #fc1e56);
  text-align: center;
  line-height: 56rpx;
  border-radius: 30rpx;
  margin: 0 auto;
  margin-top: 30rpx;
}

.shop_content {
  font-size: 0;
  background: #fff;
  padding: 50rpx 0;
  text-align: center;
}
.shop_content .shop_info {
  padding: 0 110rpx;
  font-size: 28rpx;
  color: #666;
  line-height: 42rpx;
  background: #fff;
}
.shop_content .shop_info text {
  display: block;
  font-size: 32rpx;
  color: #000;
  line-height: 52rpx;
  margin-bottom: 38rpx;
  text-align: left;
}
.shop_content .item_info image {
  width:100%;
}
.shop_content .item_info .info_name {
  font-size: 48rpx;
  color: #333;
  margin-top: 38rpx;
  margin-bottom: 18rpx;
  text-align: center;
  padding: 0 22rpx;
}
.shop_content .item_info .info_btn {
  display: inline-block;
  height: 48rpx;
  padding: 0 20rpx;
  font-size: 26rpx;
  color: #f20c59;
  border: .5px solid #f20c59;
  border-radius: 48rpx;
  box-sizing: border-box;
  text-align: center;
  line-height: 48rpx;
  margin: 0 auto;
  margin-bottom: 30rpx;
}
.shop_content .item_info .info_des {
  font-size: 31rpx;
  color: #000;
  line-height: 52rpx;
  padding: 0 22rpx;
  margin-bottom: 38rpx;
  text-align: left;
}
.detail_footer{
  background: #f2f2f2;
}
.detail_footer text{
  display: block;
  font-size: 26rpx;
  color: #999;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  padding-bottom: 140rpx;
}
.goHome{
  position:fixed;
  bottom:180rpx;
  right:20rpx;
  height: 80rpx;
  width: 80rpx;
}
.goHome image{
  width: 100%;
}